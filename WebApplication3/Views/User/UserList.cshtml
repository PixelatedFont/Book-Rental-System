@model IEnumerable<WebApplication3.Models.UserInfoTable>

@{
    ViewBag.Title = "UserList";
}

<h2>UserList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.U_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.U_PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.U_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserTable.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.U_Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.U_PhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.U_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserTable.UserName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UserID }) |
            @Html.ActionLink("Detail", "Detail", new { id=item.UserID }) |
            @Html.ActionLink("Remove", "Remove", new { id=item.UserID })
        </td>
    </tr>
}

</table>
